{
    "artifact": {
        "name": "cdap-data-pipeline",
        "version": "6.0.0",
        "scope": "SYSTEM",
        "label": "Data Pipeline - Batch"
    },
    "description": "",
    "name": "kafka-bq-demo",
    "config": {
        "resources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "driverResources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "connections": [
            {
                "from": "Kafka Consumer",
                "to": "BigQuery"
            }
        ],
        "comments": [],
        "postActions": [],
        "properties": {},
        "processTimingEnabled": true,
        "stageLoggingEnabled": true,
        "stages": [
            {
                "name": "Kafka Consumer",
                "plugin": {
                    "name": "Kafka",
                    "type": "batchsource",
                    "label": "Kafka Consumer",
                    "artifact": {
                        "name": "kafka-plugins",
                        "version": "2.1.0-0.10.2.0",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"_id\",\"type\":\"string\"},{\"name\":\"guid\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"},{\"name\":\"isActive\",\"type\":\"boolean\"},{\"name\":\"balance\",\"type\":\"string\"},{\"name\":\"picture\",\"type\":\"string\"},{\"name\":\"index\",\"type\":\"int\"},{\"name\":\"first\",\"type\":\"string\"},{\"name\":\"last\",\"type\":\"string\"},{\"name\":\"company\",\"type\":\"string\"}]}",
                        "format": "csv",
                        "kafkaBrokers": "${KAFKA_BROKERS}",
                        "referenceName": "kakfa-consumer",
                        "topic": "${KAFKA_TOPIC}"
                    }
                },
                "outputSchema": [
                    {
                        "name": "etlSchemaBody",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"_id\",\"type\":\"string\"},{\"name\":\"guid\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"},{\"name\":\"isActive\",\"type\":\"boolean\"},{\"name\":\"balance\",\"type\":\"string\"},{\"name\":\"picture\",\"type\":\"string\"},{\"name\":\"index\",\"type\":\"int\"},{\"name\":\"first\",\"type\":\"string\"},{\"name\":\"last\",\"type\":\"string\"},{\"name\":\"company\",\"type\":\"string\"}]}"
                    }
                ]
            },
            {
                "name": "BigQuery",
                "plugin": {
                    "name": "BigQueryTable",
                    "type": "batchsink",
                    "label": "BigQuery",
                    "artifact": {
                        "name": "google-cloud",
                        "version": "0.12.2",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "project": "${GCP_PROJECT_ID}",
                        "allowSchemaRelaxation": "true",
                        "serviceFilePath": "${GCP_SERVICE_ACCT}",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"_id\",\"type\":\"string\"},{\"name\":\"guid\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"},{\"name\":\"isActive\",\"type\":\"boolean\"},{\"name\":\"balance\",\"type\":\"string\"},{\"name\":\"picture\",\"type\":\"string\"},{\"name\":\"index\",\"type\":\"int\"},{\"name\":\"first\",\"type\":\"string\"},{\"name\":\"last\",\"type\":\"string\"},{\"name\":\"company\",\"type\":\"string\"}]}",
                        "referenceName": "${BQ_REF_NAME}",
                        "dataset": "${GCP_BQ_DATASET}",
                        "table": "${GCP_BQ_DATASET_TBL}"
                    }
                },
                "outputSchema": [
                    {
                        "name": "etlSchemaBody",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"_id\",\"type\":\"string\"},{\"name\":\"guid\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"},{\"name\":\"isActive\",\"type\":\"boolean\"},{\"name\":\"balance\",\"type\":\"string\"},{\"name\":\"picture\",\"type\":\"string\"},{\"name\":\"index\",\"type\":\"int\"},{\"name\":\"first\",\"type\":\"string\"},{\"name\":\"last\",\"type\":\"string\"},{\"name\":\"company\",\"type\":\"string\"}]}"
                    }
                ],
                "inputSchema": [
                    {
                        "name": "Kafka Consumer",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"_id\",\"type\":\"string\"},{\"name\":\"guid\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"},{\"name\":\"isActive\",\"type\":\"boolean\"},{\"name\":\"balance\",\"type\":\"string\"},{\"name\":\"picture\",\"type\":\"string\"},{\"name\":\"index\",\"type\":\"int\"},{\"name\":\"first\",\"type\":\"string\"},{\"name\":\"last\",\"type\":\"string\"},{\"name\":\"company\",\"type\":\"string\"}]}"
                    }
                ]
            }
        ],
        "schedule": "0 * * * *",
        "engine": "spark",
        "numOfRecordsPreview": 100,
        "maxConcurrentRuns": 1
    }
}